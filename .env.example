# WordPress Environment Configuration for Vercel + PlanetScale
# Copy this file to .env.local for local development
# Set these as environment variables in Vercel dashboard for production

# Database Configuration (Neon)
DATABASE_NAME=netjag_wordpress
DATABASE_USERNAME=your_neon_username
DATABASE_PASSWORD=your_neon_password
DATABASE_HOST=your_neon_host

# WordPress Site URLs (update with your Vercel domain)
WP_HOME=https://your-vercel-domain.vercel.app
WP_SITEURL=https://your-vercel-domain.vercel.app

# WordPress Security Keys (generate new ones for production)
AUTH_KEY=your_auth_key_here
SECURE_AUTH_KEY=your_secure_auth_key_here
LOGGED_IN_KEY=your_logged_in_key_here
NONCE_KEY=your_nonce_key_here
AUTH_SALT=your_auth_salt_here
SECURE_AUTH_SALT=your_secure_auth_salt_here
LOGGED_IN_SALT=your_logged_in_salt_here
NONCE_SALT=your_nonce_salt_here
WP_CACHE_KEY_SALT=your_cache_key_salt_here

# Optional: Custom configurations
WP_DEBUG=false
WP_DEBUG_LOG=false
WP_DEBUG_DISPLAY=false